<%- include('partials/header') -%>
    <h1>Fill up your details</h1>
    <form action="/audiDetailsInput" method="post">
        <label for="Name">Name</label>
        <input type="text" placeholder="Your Name" name="AudiName"><br><br>

        <label for="Email Id">Email Id</label>
        <input type="email" placeholder="Email Id" name="email"><br><br>

        <label for="Phone Number">Phone Number</label>
        <input type="tel" placeholder="Phone number" name="ph_num"><br><br>

        <label for="Age">Age</label>
        <input type="number" min="1" placeholder="Age" name="age"><br><br>

        <label for="Address">Address</label>
        <input type="text" placeholder="Address" name="address"><br><br>

        <input style="display: none;" value=<%= foundEvent[0]._id%> name="id">
   

        <label for="Gender">Gender</label>
            <select name="gender">
                <option >Male</option>
                <option >Female</option>
                <option >Others</option>
            </select><br></br>
        

        
         <h1><%= remain %></h1>
        <label for="tickets">How many tickets ? </label>
        <input type="number" min="1" max="<%= remain %>" placeholder="No. of tickets" name=tickets><br></br>

        

        <button type="submit">Submit</button>
    </form>
</body>
</html>