<%- include('partials/header') -%>


<h1>Participants List</h1>

<div class="table">
    <div class="table-header">
      <div class="header__item"><a id="name" class="filter__link" href="#">Sl.no</a></div>
      <div class="header__item"><a id="wins" class="filter__link filter__link--number" href="#">Participant </a></div>
      <div class="header__item"><a id="draws" class="filter__link filter__link--number" href="#">Email</a></div>
      <div class="header__item"><a id="losses" class="filter__link filter__link--number" href="#">PhoneNumber</a></div>
    </div>
  <% var count=1%>
  <% passedAudience.forEach(audience => { %>
    <div class="table-content">	
      <div class="table-row">		
        <div class="table-data"><%= count++ %> </div>
        <div class="table-data"><%= audience.audiName %></div>
        <div class="table-data"><%= audience.audiEmail %></div>
        <div class="table-data"><%= audience.audiPhNum %></div>
      </div>
    </div>  
  <% }) %> 
</div>

<%- include('partials/footer') -%>
